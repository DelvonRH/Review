
public class Review {

	public static void main(String[] args) {
		int number = 10;
		
		System.out.println("Number Before Adding Five Times: " + number);
		
		number = number + 1;
		number = number + 1;
		number = number + 1;
		number = number + 1;
		number = number + 1;

		
		System.out.println("Number After Adding Five Times: " + number);
		
//		System.out.println((number));
		
	}
	
